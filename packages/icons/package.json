{
  "name": "@drawbotics/icons",
  "version": "0.0.0",
  "description": "Drawbotics Icons Collection",
  "author": "nicmosc <nmoscholios@gmail.com>",
  "homepage": "https://github.com/Drawbotics/drylus#readme",
