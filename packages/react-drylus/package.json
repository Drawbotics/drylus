{
  "name": "@drawbotics/react-drylus",
  "version": "6.2.0-alpha.2",
  "description": "React components for the Drawbotics' ecosystem",
  "author": "nicmosc <nmoscholios@gmail.com>",
  "license": "ISC",
  "main": "./lib/bundle.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib/ tmp/",
    "build:bundle": "tsc && babel --copy-files --ignore '**/__tests__/' --root-mode upward tmp/ --out-dir lib/",
    "watch": "npm run clean && NODE_ENV=development npm run build:bundle",
    "build": "npm run clean && NODE_ENV=production npm run build:bundle",
    "build:types": "node scripts/build-types.js",
    "test": "NODE_ICU_DATA=../../node_modules/full-icu jest",
    "test:watch": "NODE_ICU_DATA=../../node_modules/full-icu jest --watchAll",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@drawbotics/drylus-style-vars": "file:../drylus-style-vars",
    "@drawbotics/enums": "0.2.0",
    "@drawbotics/icons": "file:../icons",
    "@drawbotics/use-screen-size": "^2.0.0",
    "@emotion/core": "^10.0.21",
    "@turf/circle": "^6.0.1",
    "animejs": "^3.0.1",
    "core-js": "^2.6.5",
    "dayjs": "^1.8.19",
    "emotion": "^10.0.9",
    "mapbox-gl": "^1.3.1",
    "react-calendar": "^2.19.0",
    "react-compound-slider": "^2.4.0",
    "react-mapbox-wrapper": "^2.4.0",
    "react-transition-group": "^4.0.1",
    "uuid": "^3.3.2"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "99f6de1783059e9d24e6582f69fa2982d41a1af2"
}
