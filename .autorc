{
  "plugins": [
    "npm",
    [
      "omit-commits",
      {
        "subject": ["[skip ci]"]
      }
    ]
  ],
  "labels": [
    { "releaseType": "major", "name": "💥 Breaking - Major release" },
    { "releaseType": "minor", "name": "🚀 Enhancement - Minor change" },
    { "releaseType": "patch", "name": "🐛 Bug fix - Patch" }
  ]
}