{
  "plugins": [
    "npm",
    [
      "omit-commits",
      {
        "subject": ["[skip ci]"]
      }
    ]
  ]
  "labels": {
    "patch": {
      "title": "🐛 Bug fix - Patch"
    },
    "minor": {
      "title": "🚀 Enhancement - Minor change"
    }
    "major": {
      "title": "💥 Breaking - Major release"
    }
  }
}